#include <iostream>
#include <iomanip>
using namespace std;

int main()
{
    int x[100];
    int y[100];
    char op[100];
    double z[100];
    int i, n;

    cin >> n;

    for (i = 0; i < n; i++)
    {
        cin >> x[i];
    }

    for (i = 0; i < n; i++)
    {
        cin >> y[i];
    }

    for (i = 0; i < n; i++)
    {
        cin >> op[i];

        switch (op[i])
        {
            case '*':
                z[i] = x[i] * y[i];
                break;
            case '/':
                z[i] = (double)(x[i]) / y[i];
                break;
            case '+':
                z[i] = x[i] + y[i];
                break;
            case '-':
                z[i] = x[i] - y[i];
                break;
            default:
                cout << "Operador invÃ¡lido." << endl;
                return 1;
        }
    }



    for (i = 0; i < n; i++)
    {
        cout << x[i] << " " << op[i] << " " << y[i] << " = ";

        if (op[i] == '/')
        {
            cout << setprecision(2) << fixed << z[i] << endl;
        }
        else
        {
            cout << setprecision(0) << fixed << z[i] << endl;
        }
    }

    return 0;
}
